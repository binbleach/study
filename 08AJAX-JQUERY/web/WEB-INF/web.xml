<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <!--注册SearchName-->
    <servlet>
        <servlet-name>sn</servlet-name>
        <servlet-class>controller.SearchName</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sn</servlet-name>
        <url-pattern>/sn</url-pattern>
    </servlet-mapping>

    <!--注册SearchProvince-->
    <servlet>
        <servlet-name>sp</servlet-name>
        <servlet-class>controller.SearchProvince</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sp</servlet-name>
        <url-pattern>/sp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>san</servlet-name>
        <servlet-class>controller.SearchAllName</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>san</servlet-name>
        <url-pattern>/san</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>sc</servlet-name>
        <servlet-class>controller.SearchCity</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sc</servlet-name>
        <url-pattern>/sc</url-pattern>
    </servlet-mapping>
</web-app>
