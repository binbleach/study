<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd">


    <dubbo:application name="02dubbo-one"/>
    <!--
    id:远程服务接口对象的代理名称 userService首字母小写
    interface:远程服务接口全限定名
    url:访问服务接口的地址
    registry :直连A/N
    -->
    <dubbo:reference id="userService" interface="com.huangjiabin.service.UserService"
                     url="dubbo://localhost:20880" registry="N/A"/>

</beans>